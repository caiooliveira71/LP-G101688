Algoritmo "semnome"

Var
   idade: inteiro
   altura: caractere
   nome: caractere

Inicio


  escreva("digite seu nome: ")
  
  leia(nome)
  
  escreva("Digite sua idade: ")
  
  leia(idade)
  
  escreva("digite sua altura:")
  
  leia(altura)
  
  escreval
  
  escreval("nome:",nome)
  escreval("idade:",idade)
  escreval("altura:",altura)
  
  


Fimalgoritmo